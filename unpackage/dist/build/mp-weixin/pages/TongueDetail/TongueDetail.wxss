.home .header{background-color:#6fad8f;padding:25rpx 25rpx 60rpx;height:250rpx;width:700rpx}.home .header-text{display:flex;flex-wrap:wrap;justify-content:center;font-size:48rpx}.home .box{background-color:#fff;border-radius:25rpx;margin:-200rpx 30rpx 0;flex-wrap:wrap;justify-content:center;width:700rpx;padding:60rpx 25rpx;box-shadow:9px 10px 16px rgba(0,0,0,.5);box-sizing:border-box}.home .item{width:98%;box-sizing:border-box;margin:2rpx 0;padding:10rpx;flex-direction:row;font-weight:550;font-size:110%}.home .item-border{border:2rpx solid #6FAD8F;border-radius:5rpx}.home .item_detail{width:98%;display:flex;align-items:center;justify-content:space-between}.home .detail-text{font-size:90%;font-family:Gill Sans,Gill Sans MT,Calibri,Trebuchet MS,sans-serif;text-indent:2em}.home .slogan{font-size:9pt}
